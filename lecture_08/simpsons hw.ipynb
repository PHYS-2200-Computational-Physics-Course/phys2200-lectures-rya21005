   "source": [
    "\n",
    "function mysimpsons(fun, a, b, n)\n",
    "    h = (b - a)/(n - 1)\n",
    "    s1 = fun(a) + fun(b)\n",
    "    s4 = 0.0\n",
    "    for i = 2:2:(n-2)\n",
    "        s4 += fun(a + h(i-1))\n",
    "    end\n",
    "    return h/3*(s1+4*s4+2*s2)\n",
    "end"
   ]
  },
